{
  "name": "attendeer",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "build": "webpack build --mode=production && sass ./src/scss/main.scss ./public/css/main.css --style compressed",
    "build-and-start": "npm run build && npm run start",
    "watch:webpack": "webpack --watch",
    "watch:nodemon": "nodemon --ext js,mjs,json,njk,svg app.js --ignore src/",
    "watch:sass": "sass ./src/scss/main.scss ./public/css/main.css --watch",
    "debug": "run-p watch:webpack watch:sass watch:nodemon"
  },
  "author": "",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "body-parser": "^1.19.2",
    "boxicons": "^2.1.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-nunjucks": "^2.2.5",
    "express-session": "^1.17.2",
    "firebase": "^9.6.7",
    "firebase-admin": "^10.0.2",
    "google-auth-library": "^7.14.0",
    "html": "^1.0.0",
    "js-cookie": "^3.0.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.3",
    "sass": "^1.49.9",
    "webrtc-adapter": "^8.1.1"
  },
  "devDependencies": {
    "compression-webpack-plugin": "^10.0.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  }
}
